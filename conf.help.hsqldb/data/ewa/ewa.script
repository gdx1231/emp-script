INSERT INTO EWA_CFG VALUES('|admins|bk_base.xml','BAS_TAG.ListFrame.Modify','<EasyWebTemplate Author="admin" CreateDate="2011-05-12 17:30:35" Name="BAS_TAG.ListFrame.Modify" UpdateDate="2021-03-29 17:46:53"><Page><Name><Set Name="BAS_TAG.ListFrame.Modify"/></Name><FrameTag><Set FrameTag="ListFrame"/></FrameTag><SkinName><Set IsXhtml="0" SkinName="Test1"/></SkinName><DataSource><Set DataSource="ewademo"/></DataSource><ConfigMemo><Set><ConfigMemo/></Set></ConfigMemo><Cached><Set CachedSeconds="" CachedType=""/></Cached><Acl><Set Acl="com.gdxsoft.easyweb.demo.acl.AdminImpl"/></Acl><Log><Set Log=""/></Log><DescriptionSet><Set Info="\u5b57\u5178\u4fe1\u606f" Lang="zhcn" Memo=""/><Set Info="Dictionary information" Lang="enus" Memo=""/></DescriptionSet><PageAttributeSet><Set PageAttName="" PageAttValue=""/></PageAttributeSet><GroupSet/><Size><Set FrameCols="" HAlign="center" Height="" HiddenCaption="" VAlign="top" Width="100%"/></Size><AddHtml><Set><Top><![CDATA[ <script type="text/javascript" \u000d\u000a         src="@RV_EWA_STYLE_PATH/EWA_STYLE/js/UTILS/EWA_UT_TRANS.js"></script>\u000d\u000a<input type=button value=''\u6dfb\u52a0\u65b0\u7eaa\u5f55'' onclick="addNew()" \u000d\u000a\u0009res=''EWA.F.FOS["@SYS_FRAME_UNID"].Resources.butAdd.GetInfo()'' />\u000d\u000a<hr>\u000d\u000a\u0009\u0009\u0009\u0009]]></Top><Bottom><![CDATA[<br>]]></Bottom></Set></AddHtml><AddScript><Set><Top><![CDATA[//BAS_TAG.ListFrame.Modify\u000d\u000afunction addNew() {\u000d\u000a    var p = ''EWA_MTYPE=N&BT_ID=@BT_ID'';\u000d\u000a    var bas_tag_grp = $(''span[id=BAS_TAG_GRP]'').last();\u000d\u000a    if (bas_tag_grp.length > 0) {\u000d\u000a        var idx = $(''span[id=BAS_TAG_ORD]'').last().html();\u000d\u000a        p += "&BAS_TAG_GRP=" + bas_tag_grp.html() + "&BAS_TAG=" + bas_tag_grp.html() + "_&BAS_TAG_ORD=" + (idx * 1 + 1);\u000d\u000a    }\u000d\u000a    EWA.UI.Dialog.OpenReloadClose(''@SYS_FRAME_UNID'', ''@xmlname'', ''BAS_TAG.Frame.NewModify'', false, p);\u000d\u000a}\u000d\u000a\u000d\u000afunction show(id) {\u000d\u000a    //var w = window.parent.frames[0];\u000d\u000a    var w = window;\u000d\u000a    for (var n in w.EWA.F.FOS) {\u000d\u000a        var aa = w.EWA.F.FOS[n];\u000d\u000a        if (aa instanceof EWA_TreeClass) {\u000d\u000a            var node0 = aa.GetFocusNode();\u000d\u000a            if (node0) {\u000d\u000a                node0.Mark(false);\u000d\u000a            }\u000d\u000a            aa.ShowNode(id);\u000d\u000a            var node2 = aa.GetFocusNode();\u000d\u000a            node2.BookOpen();\u000d\u000a            node2.NodeOpen();\u000d\u000a            var o = node2.GetObject();\u000d\u000a            var top = w.$(o).offset().top;\u000d\u000a            w.document.body.scrollTop = top;\u000d\u000a            break;\u000d\u000a        }\u000d\u000a    }\u000d\u000a}]]></Top><Bottom><![CDATA[var trans;\u000d\u000a\u000d\u000afunction aa(fromObj) {\u000d\u000a    var obj1 = fromObj.parentNode.parentNode.cells[4].getElementsByTagName(''span'')[0];\u000d\u000a    if (obj1 == null) {\u000d\u000a        return;\u000d\u000a    }\u000d\u000a    if (obj1.innerHTML != '''') {\u000d\u000a        return;\u000d\u000a    }\u000d\u000a    if (trans == null) {\u000d\u000a        trans = new EWA_UT_TRANS();\u000d\u000a    }\u000d\u000a    trans.TransToEn(fromObj.innerHTML, obj1);\u000d\u000a}\u000d\u000a\u000d\u000afunction bb() {\u000d\u000a    if (idx <= ipts.length - 1) {\u000d\u000a        aa(ipts[idx]);\u000d\u000a        idx++;\u000d\u000a        setTimeout(bb, 123);\u000d\u000a        return;\u000d\u000a    }\u000d\u000a    setTimeout(cc, 1231);\u000d\u000a}\u000d\u000a\u000d\u000afunction cc() {\u000d\u000a    var ids = [];\u000d\u000a    var vals = [];\u000d\u000a    for (var i = 0; i < ipts.length; i++) {\u000d\u000a        var tr = ipts[i].parentNode.parentNode;\u000d\u000a        var span = tr.cells[4].getElementsByTagName(''span'')[0];\u000d\u000a        if (span.getAttribute(''ewa_trans_end'') == ''ok'') {\u000d\u000a            ids.push(GetInnerText(tr.cells[0]));\u000d\u000a            vals.push(GetInnerText(span));\u000d\u000a        }\u000d\u000a    }\u000d\u000a    if (ids.length == 0) {\u000d\u000a        return;\u000d\u000a    }\u000d\u000a    var u=EWA.F.FOS[''@sys_frame_unid''].Url;\u000d\u000a    var ajax = new EWA_AjaxClass();\u000d\u000a    ajax.AddParameter(''ids'', ids.join(''`''));\u000d\u000a    ajax.AddParameter(''vals'', vals.join(''`''));\u000d\u000a    ajax.AddParameter(''EWA_ACTION'', ''EXTENDACTION2'');\u000d\u000a    ajax.AddParameter(''EWA_AJAX'', ''JSON'');\u000d\u000a    ajax.PostNew(u, function() {\u000d\u000a        if (ajax.IsRunEnd()) {\u000d\u000a            $Tip(''ok'');\u000d\u000a        }\u000d\u000a    });\u000d\u000a}\u000d\u000avar ipts;\u000d\u000avar idx;\u000d\u000a\u000d\u000afunction tall() {\u000d\u000a    ipts = $F(document.body, ''span'', ''id'', ''BAS_TAG_NAME'');\u000d\u000a    idx = 0;\u000d\u000a    bb();\u000d\u000a}]]></Bottom></Set></AddScript><AddCss><Set><AddCss/></Set></AddCss><ChartsShow><Set ChartsShow=""/></ChartsShow><RedrawJson/><BoxJson/><LeftJson/><FrameHtml/><PageSize><Set AllowExport="BAS_TAG" ColSize="" IsSplitPage="1" KeyField="BAS_TAG" PageSize="50" Recycle="" StatusField=""/></PageSize><ListUI><Set luAddPreRowFunc="" luButtons="1" luDblClick="1" luDblClickIdx="2" luSearch="1" luSelect="S"/></ListUI><MenuShow/><Menu/><Tree/><HtmlFrame><Set FrameBorder="" FrameSize="" FrameSubUrl="" FrameType=""/></HtmlFrame><TreeIconSet/><MGAxisX/><MGAxisY/><MGCell/><LogicShow/></Page><Action><ActionSet><Set IsPostMsg="no" LogMsg="" Type="OnPageLoad"><CallSet><Set CallIsChk="" CallName="OnPageLoad SQL" CallType="SqlSet" Test=""/></CallSet></Set><Set IsPostMsg="no" LogMsg="" Type="OnFrameDelete"><CallSet><Set CallIsChk="" CallName="OnFrameDelete SQL" CallType="SqlSet" Test=""/></CallSet></Set><Set IsPostMsg="no" LogMsg="" Type="OnListFrameUpdateCell"><CallSet><Set CallIsChk="" CallName="ExtendAction0 SQL" CallType="SqlSet" Test=