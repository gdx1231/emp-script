<EwaDefine>
	<Steps>
		<Step AfterEval="this.CreateMainInfo();">
			<DescriptionSet>
				<Set Lang="zhcn" Info="第一步：从数据库树选择一个表" />
				<Set Lang="enus" Info="First,select table" />
			</DescriptionSet>
		</Step>
		<Step Eval="this.SelectType()">
			<DescriptionSet>
				<Set Lang="zhcn" Info="第二步：选择类型" />
				<Set Lang="enus" Info="First: select table" />
			</DescriptionSet>
		</Step>
