	<Valids>
		<Valid Name="" Regex="">
			<DescriptionSet>
				<Set Lang="zhcn" Info="" Memo="" />
				<Set Lang="enus" Info="" Memo="" />
			</DescriptionSet>
		</Valid>
		<Valid Name="Email" Regex="/^$|^\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/">
			<DescriptionSet>
				<Set Lang="zhcn" Info="电子邮件" Memo="请用aa@bb.cn格式" />
				<Set Lang="enus" Info="Email" Memo="Pls using name@gdxsoft.com style" />
			</DescriptionSet>
		</Valid>
		<Valid Name="Alpha" Regex="/^[\w]*$/g">
			<DescriptionSet>
				<Set Lang="zhcn" Info="字符" Memo="只能由数字和字母组成" />
				<Set Lang="enus" Info="字符" Memo="only numbers or letters" />
			</DescriptionSet>
		</Valid>
		<Valid Name="Digit" Regex="/^$|^[-+]?\d+(\.\d*)?$/">
			<DescriptionSet>
				<Set Lang="zhcn" Info="数字" Memo="只能由数字组成" />
				<Set Lang="enus" Info="数字" Memo="only numbers" />
			</DescriptionSet>
		</Valid>
		<Valid Name="Duration" Regex="/^$|^(\d{1,2}:)?[0-5][0-9]:[0-5][0-9]$/">
			<DescriptionSet>
				<Set Lang="zhcn" Info="持续时间" Memo="格式错误" />
				<Set Lang="enus" Info="持续时间" Memo="Format error" />
			</DescriptionSet>
		</Valid>
		<Valid Name="Mobile" Regex="/^$|^(13|14|15|16|17|18|19)([0-9]{9})$/">
			<DescriptionSet>
				<Set Lang="zhcn" Info="手机" Memo="手机格式错误" />
				<Set Lang="enus" Info="手机" Memo="Format error" />
			</DescriptionSet>
		</Valid>
		<Valid Name="Phone"
			Regex="/^$|^((\+?[0-9]{2,4}\-[0-9]{3,4}\-)|([0-9]{3,4}\-))?([0-9]{7,8})(\-[0-9]+)?$/">
			<DescriptionSet>
				<Set Lang="zhcn" Info="座机" Memo="座机格式错误，区号或分机用-分隔" />
				<Set Lang="enus" Info="座机" Memo="Format error" />
			</DescriptionSet>
		</Valid>
		<Valid Name="Tele"
			Regex="/^$|^(13|14|15|16|17|18|19)([0-9]{9})$|^((\+?[0-9]{2,4}\-[0-9]{3,4}\-)|([0-9]{3,4}\-))?([0-9]{7,8})(\-[0-9]+)?$/">
			<DescriptionSet>
				<Set Lang="zhcn" Info="电话" Memo="电话格式错误,请填写手机或座机（区号或分机用-分隔）" />
				<Set Lang="enus" Info="电话" Memo="Format error" />
			</DescriptionSet>
		</Valid>
		<Valid Name="IDCard" Regex="/^$|^\d{14}(\d{1}|\d{4}|(\d{3}[xX]))$/">
			<DescriptionSet>
				<Set Lang="zhcn" Info="身份证" Memo="身份证错误" />
				<Set Lang="enus" Info="身份证" Memo="Format error" />
			</DescriptionSet>
		</Valid>
		<Valid Name="Date"
			Regex="/^$|^((((1[6-9]|[2-9]\d)\d{2})-(0?[13578]|1[02])-(0?[1-9]|[12]\d|3[01]))|(((1[6-9]|[2-9]\d)\d{2})-(0?[13456789]|1[012])-(0?[1-9]|[12]\d|30))|(((1[6-9]|[2-9]\d)\d{2})-0?2-(0?[1-9]|1\d|2[0-9]))|(((1[6-9]|[2-9]\d)(0[48]|[2468][048]|[13579][26])|((16|[2468][048]|[3579][26])00))-0?2-29-))$/">
			<DescriptionSet>
				<Set Lang="zhcn" Info="日期" Memo="日期格式错误" />
				<Set Lang="enus" Info="日期" Memo="Format error" />
			</DescriptionSet>
		</Valid>
		<Valid Name="YYYY_YYYY" Regex="/^\d{4}\-\d{4}$/gi">
			<DescriptionSet>
				<Set Lang="zhcn" Info="从那年到那年" Memo="格式：2014-2018" />
				<Set Lang="enus" Info="从那年到那年" Memo="e.g.：2014-2018" />
			</DescriptionSet>
		</Valid>
		<Valid Name="YYYYMMDD" Regex="/^([1-2]\d{3})(0?[1-9]|10|11|12)([1-2]?[0-9]|0[1-9]|30|31)$/gi">
			<DescriptionSet>
				<Set Lang="zhcn" Info="8位日期" Memo="格式：20140131" />
				<Set Lang="enus" Info="8位日期" Memo="e.g.：20140131" />
			</DescriptionSet>
		</Valid>
		<Valid Name="YYYYMM" Regex="/^([1-2]\d{3})(0?[1-9]|10|11|12)$/gi">
			<DescriptionSet>
				<Set Lang="zhcn" Info="6位年月" Memo="格式：201408" />
				<Set Lang="enus" Info="6位年月" Memo="e.g.：201408" />
			</DescriptionSet>
		</Valid>
		<Valid Name="HH_MM" Regex="/^$|^(\d{1}|0\d{1}|1\d{1}|2[0-3]):[0-5]\d{1}$/">
			<DescriptionSet>
				<Set Lang="zhcn" Info="时间 HH:MM" Memo="格式：09:18 或 9:18" />
				<Set Lang="enus" Info="Time HH:MM" Memo="e.g.：09:18 or 9:18" />
			</DescriptionSet>
		</Valid>
		<Valid Name="HH_MM_d4"
			Regex="/^$|^(\d{1}|0\d{1}|1\d{1}|2[0-3]):[0-5]\d{1}$|^(0\d{1}|1\d{1}|2[0-3])[0-5]\d{1}$/">
			<DescriptionSet>
				<Set Lang="zhcn" Info="时间 HH:MM,4位数字需自己处理格式" Memo="格式：09:18 或 9:18 或 0918" />
				<Set Lang="enus" Info="Time HH:MM" Memo="e.g.：09:18 or 9:18 or 0918" />
			</DescriptionSet>
		</Valid>
	</Valids>
</Globals>