	<SqlTemplates des="用于不同数据库的 SQL 模板">
		<SqlTemplate Name="MSSQL">
			<PrimaryKey>
				<![CDATA[
ALTER TABLE {TABLE_NAME} ADD CONSTRAINT PK_{TABLE_NAME} PRIMARY KEY CLUSTERED(
	{FIELD_NAMES}
)
				]]>
			</PrimaryKey>
			<FieldCommentsGet>
				<![CDATA[
SELECT objname COLUMN_NAME, convert(varchar(8000),value) REMARKS FROM (
	SELECT * FROM ::fn_listextendedproperty(NULL, 'user', '{SCHMEA}', 'table', '{TABLE_NAME}', 'column', null) 
	union 
	SELECT * FROM ::fn_listextendedproperty(NULL, 'user', '{SCHMEA}', 'view', '{TABLE_NAME}', 'column', null)
) gggg
				]]>
			</FieldCommentsGet>
			<FieldCommentSet>
				<![CDATA[
if exists(SELECT * FROM 
	::fn_listextendedproperty(NULL, 'user', '{SCHMEA}','table', '{TABLE_NAME}', 'column', '{FIELD_NAME}'))
  BEGIN
  	exec sp_updateextendedproperty 'MS_Description', '{COMMENT}', 'user', '{SCHMEA}', 'table', '{TABLE_NAME}' , 'column', '{FIELD_NAME}'
  END
ELSE
  BEGIN
	exec sp_addextendedproperty 'MS_Description', '{COMMENT}', 'user', '{SCHMEA}', 'table', '{TABLE_NAME}' , 'column', '{FIELD_NAME}'
  END
]]>
			</FieldCommentSet>
			<ViewDDL><![CDATA[exec sp_helptext '{SCHMEA}.{TABLE_NAME}']]></ViewDDL>
		</SqlTemplate>
		<SqlTemplate Name="ORACLE">
			<PrimaryKey>
				<![CDATA[
ALTER TABLE {TABLE_NAME} ADD CONSTRAINT PK_{TABLE_NAME} PRIMARY KEY (
	{FIELD_NAMES}
) USING INDEX

				]]>
			</PrimaryKey>
			<FieldCommentsGet>
				<![CDATA[
SELECT A.COLUMN_NAME ,A.data_length Column_SIZE,A.Data_type Type_NAME, 
		CASE 
			WHEN a.NULLABLE='Y' THEN 'YES' 
			ELSE 'NO' 
		END AS IS_NULLABLE, 
		C.COMMENTS REMARKS,cc.table_name AS FK_TABLE_NAME, 
		cc.column_name as FK_COLUMN_NAME  
FROM dba_tab_columns A 
 	left join dba_COL_COMMENTS C ON C.column_name=A.column_name AND A.table_name=C.table_name
    left join 
    ( 
    	select c2.table_name,c3.column_name from dba_constraints c1 
    		left join dba_constraints c2 on c1.r_constraint_name=c2.constraint_name
    		left join dba_cons_columns c3 on c3.constraint_name=c1.constraint_name
    		
    		where c1.constraint_type='R' and c1.table_name='{TABLE_NAME}' 
    			and c1.owner='{SCHMEA}'
    ) cc on cc.column_name=a.COLUMN_NAME
    WHERE A.TABLE_NAME='{TABLE_NAME}' AND A.OWNER=C.OWNER AND A.OWNER='{SCHMEA}' order by a.COLUMN_ID
]]></FieldCommentsGet>
			<FieldCommentSet>
				<![CDATA[COMMENT ON COLUMN {TABLE_NAME}.{FIELD_NAME} IS '{COMMENT}']]>
			</FieldCommentSet>
		</SqlTemplate>
		<SqlTemplate Name="MYSQL">
			<PrimaryKey>
				<![CDATA[ALTER TABLE {TABLE_NAME} ADD PRIMARY KEY ({FIELD_NAMES})]]>
			</PrimaryKey>
			<IdentityField des="获取字段是否为自增型">
				<![CDATA[SELECT TABLE_NAME,COLUMN_NAME FROM information_schema.columns where table_schema='{SCHMEA}' AND 	TABLE_NAME='{TABLE_NAME}' AND EXTRA='auto_increment']]>
			</IdentityField>
			<ViewDDL><![CDATA[SHOW CREATE TABLE `{SCHMEA}`.`{TABLE_NAME}`]]></ViewDDL>
			<TableDDL><![CDATA[SHOW CREATE TABLE `{SCHMEA}`.`{TABLE_NAME}`]]></TableDDL>
		</SqlTemplate>

		<SqlTemplate Name="HSQLDB">
			<FieldCommentSet>
				<![CDATA[COMMENT ON COLUMN {TABLE_NAME}.{FIELD_NAME} IS '{COMMENT}']]>
			</FieldCommentSet>
		</SqlTemplate>
	</SqlTemplates>

