	var s="";
	var tbs=window.frames[0].document.getElementsByTagName('table');
	var m=0;
	var firstTb;
	for(var i=0;i<tbs.length;i++){
		var tb=tbs[i];
		if(tb.id.indexOf('TABLE#') !=0){
			continue;
		}
		if(m==0){
			firstTb=tb;
			m=1;
		} 
		var chks=tb.getElementsByTagName('input');
		for(var j=0;j<chks.length;j++){
			var chk=chks[j];
			if(chk.checked){
				if(s.length >0){
					s+=", ";
				}
				s+=chk.parentNode.parentNode.id.replace('ROW#','').replace('#','.');
			}
		}
	}
	s='SELECT ' + s + ' FROM '+firstTb.id.replace('TABLE#','');
	var tb=firstTb;
	var tb1=null;
	var kk;
	links={};
	
	while((kk=getLink2( tb, tb1)) !=null){
		var tr=kk.loc==1?kk.LinkObj2:kk.LinkObj1;
		var tr1=kk.loc==1?kk.LinkObj1:kk.LinkObj2;
		
		var tb=tr.parentNode.parentNode;
		var tb1=tr1.parentNode.parentNode;
		s+='\r\n\t JOIN ' +tb.id.replace('TABLE#','')
			+' ON (' + kk.LinkObj2.id.replace('ROW#','').replace('#','.') 
			+ ' = ' + kk.LinkObj1.id.replace('ROW#','').replace('#','.') + ')';
		m++;
		if(m>100)
			break;
	}
	
	$X('sql').value=s;
}
var links={};
function getLink2(tb,tb1){
	var k=getLink(tb);
	if(k==null && tb1 !=null){
		k=getLink(tb1);
	}
	return k;
}
function getLink(tb){
	for(var i=0;i<window.frames[0].links.length;i++){
		var k=window.frames[0].links[i];
	
		if(k==null){
			continue;
		}
		var o1=k.LinkObj1.parentNode.parentNode;
		var o2=k.LinkObj2.parentNode.parentNode;
		if(o1==tb || o2==tb){
			var id=o1.id+'@@'+o2.id;
			var id1=o2.id+'@@'+o1.id;
			if(links[id]==null && links[id1]==null){
				links[id]=1;
				k.loc= (o1==tb ? 1 : 2);
				return k;
			}
		}
	}
	
	return null;
}]]></Top><Bottom><![CDATA[document.body.style.overflow='hidden'; ]]></Bottom></Set></AddScript><AddCss><Set><AddCss><![CDATA[body{overflow:hidden;}]]></AddCss></Set></AddCss><ChartsShow/><FrameHtml><Set><FrameHtml/></Set></FrameHtml><PageSize/><MenuShow/><Menu/><Tree/><HtmlFrame><Set FrameBorder="" FrameSize="50%,*" FrameType=""/></HtmlFrame><TreeIconSet/></Page><Action><ActionSet/><SqlSet/><ClassSet/><XmlSet/><XmlSetData/><ScriptSet/><UrlSet/><CSSet/><CallSet/></Action><XItems/><Menus/><Charts/><PageInfos/></EasyWebTemplate><EasyWebTemplate CreateDate="2012-02-14 17:14:33" Name="tables_same" UpdateDate="2015-03-30 18:20:21"><Page><FrameTag><Set FrameTag="ListFrame"/></FrameTag><Name><Set Name="tables_same"/></Name><SkinName><Set IsXhtml="" SkinName="Test1"/></SkinName><DataSource><Set DataSource=""/></DataSource><Cached/><Acl><Set Acl="com.gdxsoft.easyweb.define.DefineAcl"/></Acl><Log><Set Log=""/></Log><DescriptionSet><Set Info="表/试图一致性 cfgFrom, cfgTo" Lang="zhcn" Memo=""/><Set Info="Table/trying to consistency cfgFrom, cfgTo" Lang="enus" Memo=""/></DescriptionSet><PageAttributeSet/><GroupSet/><Size><Set FrameCols="" HAlign="" Height="" HiddenCaption="" VAlign="" Width="100%"/></Size><AddHtml><Set><Top><![CDATA[ ]]></Top><Bottom/></Set></AddHtml><AddScript><Set><Top/><Bottom><![CDATA[var xmlName="@VIEW_XMLNAME";     
