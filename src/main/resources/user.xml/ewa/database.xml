function link(t1){
	var tbT1=t1.parentNode.parentNode.parentNode;
	
	var tbs=document.getElementsByTagName('table');
	for(var i=0;i<tbs.length;i++){
		var tb=tbs[i];
		if(tb.id.indexOf('TABLE#') !=0){
			continue;
		}
